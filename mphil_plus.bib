@article{MiyagawaS.etal_2019,
  title = {Inducing and Blocking Labeling},
  author = {Miyagawa, Shigeru and Wu, Danfeng and Koizumi, Masatoshi},
  date = {2019-12-31},
  journaltitle = {Glossa: a journal of general linguistics},
